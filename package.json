{
  "name": "nest-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "rimraf dist && webpack",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "server": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --daemon",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "starthmr": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "pkg:mac": "rimraf ./release/app-mac && pkg . -t node14-mac-x64 -o ./release/app-mac",
    "pkg:linux": "rimraf ./release/app-linux && pkg . -t node14-linux-x64 -o ./release/app-linux"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@types/multer": "^1.4.7",
    "@types/sequelize": "^4.28.14",
    "ali-oss": "^6.17.1",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.7.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nunjucks": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize-typescript": "^2.1.3",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/ali-oss": "^6.16.5",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pkg":{
    "assets": [
      "./public/**/*",
      "./views/**/*"
    ]
  }
}
